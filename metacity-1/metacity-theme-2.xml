<?xml version="1.0"?>
<!--This theme is a combination of 'Carbonated for Adwaita' and 'Knopka' by Resomizer-->
<metacity_theme>
<info>
  <name>Ink</name>
  <author>etnbrd</author>
  <copyright>1inuxr</copyright>
  <date>January 29 2014</date>
  <description>Ink : minimal flat lined theme</description>
</info>

<!-- general window layout -->
<frame_geometry name="frame_geometry_normal" title_scale="medium" rounded_top_left="false" rounded_top_right="false" rounded_bottom_left="false" rounded_bottom_right="false">
  <distance name="left_width" value="0"/>
  <distance name="right_width" value="0"/>
  <distance name="bottom_height" value="0"/>
  <distance name="left_titlebar_edge" value="0"/>
  <distance name="right_titlebar_edge" value="0"/>
  <distance name="button_width" value="15"/>
  <distance name="button_height" value="15"/>
  <distance name="title_vertical_pad" value="0"/>
  <border name="title_border" left="15" right="8" top="8" bottom="8"/>
  <border name="button_border" left="8" right="13" top="8" bottom="8"/>
</frame_geometry>

<frame_geometry name="frame_geometry_abnormal" title_scale="medium" rounded_top_left="false" rounded_top_right="false">
  <distance name="left_width" value="0"/>
  <distance name="right_width" value="0"/>
  <distance name="bottom_height" value="0"/>
  <distance name="left_titlebar_edge" value="10"/>
  <distance name="right_titlebar_edge" value="10"/>
  <distance name="button_width" value="34"/>
  <distance name="button_height" value="32"/>
  <distance name="title_vertical_pad" value="0"/>
  <border name="title_border" left="8" right="8" top="3" bottom="0"/>
  <border name="button_border" left="0" right="0" top="0" bottom="0"/>
</frame_geometry>

<frame_geometry name="geometry_maximized" has_title="false" rounded_top_left="false" rounded_top_right="false" rounded_bottom_left="false" rounded_bottom_right="false">
  <distance name="left_width" value="0"/>
  <distance name="right_width" value="0"/>
  <distance name="bottom_height" value="0"/>
  <distance name="left_titlebar_edge" value="0"/>
  <distance name="right_titlebar_edge" value="0"/>
  <distance name="button_width" value="0"/>
  <distance name="button_height" value="0"/>
  <distance name="title_vertical_pad" value="0"/>
  <border name="title_border" left="8" right="8" top="0" bottom="0"/>
  <border name="button_border" left="0" right="0" top="0" bottom="0"/>
</frame_geometry>

<frame_geometry name="border" has_title="false">
  <distance name="left_width" value="30"/>
  <distance name="right_width" value="30"/>
  <distance name="bottom_height" value="30"/>
  <distance name="left_titlebar_edge" value="0"/>
  <distance name="right_titlebar_edge" value="0"/>
  <distance name="button_width" value="0"/>
  <distance name="button_height" value="0"/>
  <distance name="title_vertical_pad" value="0"/>
  <border name="title_border" left="8" right="8" top="3" bottom="0"/>
  <border name="button_border" left="0" right="0" top="0" bottom="0"/>
</frame_geometry>


<!-- title text -->
<draw_ops name="draw_title_text_normal">
  <title color="rgb(255, 255, 255)" x="0" y="0"/>
</draw_ops>

<draw_ops name="draw_title_text_unfocused">
  <title color="rgb(120, 120, 120)" x="0" y="0"/>
</draw_ops>


<!-- title -->
<!-- if using height="object_height" then won't scale as title text gets larger --> 
<draw_ops name="draw_title">
  <rectangle color="rgb(23, 23, 23)" x="0" y="0" width="width" height="height" filled="true" />
</draw_ops>

<draw_ops name="draw_title_utility">
  <include name="draw_title" />
</draw_ops>

<draw_ops name="draw_title_unfocused">
  <rectangle color="rgb( 0, 0, 0)" x="0" y="0" width="width" height="height" filled="true" />
</draw_ops>

<draw_ops name="draw_title_utility_unfocused">
  <include name="draw_title_unfocused" />
</draw_ops>

<draw_ops name="draw_title_maximized">
  <include name="draw_title" />
</draw_ops>

<draw_ops name="draw_title_maximized_unfocused">
  <include name="draw_title_unfocused" />
</draw_ops>

<!-- window frames -->
<draw_ops name="draw_frame">
  <line color="rgb(23, 23, 23)" x1="width-1" x2="width-1" y1="0" y2="height"/>
</draw_ops>

<draw_ops name="draw_frame_utility">
  <line color="rgb(23, 23, 23)" x1="width-1" x2="width-1" y1="0" y2="height"/>
</draw_ops>

<draw_ops name="bottom_edge">
  <line color="rgb(23, 23, 23)" x1="0" x2="width" y1="height-1" y2="height-1"/>
</draw_ops>

